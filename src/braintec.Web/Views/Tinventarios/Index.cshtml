@model IEnumerable<braintec.Infrastructure.Data.Tinventario>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead class="thead-dark">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nombrerepuesto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Marcarepuesto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cantidadexistente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Tarifarepuesto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdcategoriaNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdproveedorNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nombrerepuesto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Marcarepuesto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cantidadexistente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tarifarepuesto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdcategoriaNavigation.Idcategoria)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdproveedorNavigation.Idproveedor)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Refrepuesto">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Refrepuesto">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Refrepuesto">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
