@model IEnumerable<braintec.Infrastructure.Data.Tproveedor>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Rsocialproveedor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Percontactoproveedor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Telefonoproveedor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Correoproveedor)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Rsocialproveedor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Percontactoproveedor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Telefonoproveedor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Correoproveedor)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Idproveedor">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Idproveedor">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Idproveedor">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
